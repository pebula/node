{
  "name": "docs-tom",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/docs/tom/src",
  "projectType": "app",
  "tags": ["docs"],
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": [
          "apps/docs/tom/**/*.{ts,tsx,js,jsx}",
          "apps/docs/tom/package.json"
        ]
      }
    },
    "start": {
      "executor": "@pebula/nx-docusaurus:start",
      "options": { }
    },
    "build": {
      "executor": "@pebula/nx-docusaurus:build",
      "inputs": [
        "{projectRoot}/docs/*.(md|mdx)",
        "{projectRoot}/docs/**/*.(md|mdx)"
      ],
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/gh-pages/{projectName}"
      }
    },
    "serve": {
      "executor": "@pebula/nx-docusaurus:serve",
      "inputs": [
        "{projectRoot}/docs/*.(md|mdx)",
        "{projectRoot}/docs/**/*.(md|mdx)"
      ],
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/gh-pages/{projectName}"
      }
    },
    "gh-pages": {
      "options": {
        "taskOptions": {
          "gh-pages": {
            "args": [ "tom" ]
          }
        }
      }
    }
  }
}
