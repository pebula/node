{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  "mainEntryPointFilePath": "<projectFolder>/cjs/index.d.ts",
  "docModel": {
    "enabled": false
  },
  "apiReport": {
    "enabled": true,
    "reportFolder": "<lernaFolder>/reviews/api",
    "reportTempFolder": "<lernaFolder>/reviews/api/temp"
  },
  "dtsRollup": {
    "enabled": true,
    "untrimmedFilePath": "",
    "publicTrimmedFilePath": "<projectFolder>/cjs/index-public.d.ts"
  },
  "tsdocMetadata": {
    "enabled": false
  },
  "messages": {
    "tsdocMessageReporting": {
      "default": {
        "logLevel": "none"
      }
    },
    "extractorMessageReporting": {
      "ae-missing-release-tag": {
        "logLevel": "none"
      },
      "ae-unresolved-link": {
        "logLevel": "none"
      }
    }
  }
}
