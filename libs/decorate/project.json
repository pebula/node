{
  "name": "decorate",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/decorate/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/decorate"],
      "options": {
        "outputPath": "coverage/libs/decorate",
        "jestConfig": "libs/decorate/jest.config.ts"
      }
    },
    "build": {
      "executor": "./dist/libs/nx-build-pipe:build",
      "options": {
        "task": {
          "type": "group",
          "composition": "sequence",
          "pipe": [
            {
              "type": "target",
              "target": "decorate:package"
            }
          ]
        }
      }
    },
    "package": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/decorate",
        "tsConfig": "libs/decorate/tsconfig.lib.json",
        "main": "libs/decorate/src/index.ts",
        "assets": ["libs/decorate/*.md"],
        "srcRootForCompilationRoot": "libs/decorate"
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "syncVersions": false
      }
    },
    "apiExtractor": {
      "executor": "./dist/libs/nx-api-extractor:build",
      "options": {
        "buildTargetName": "package",
        "baseConfigFile": "./api-extractor.base.json"
      }
    }
  }
}
