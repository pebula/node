{
  "name": "decorate",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/decorate/src",
  "projectType": "library",
  "tags": ["release"],
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": [
          "libs/decorate/**/*.{ts,tsx,js,jsx}",
          "libs/decorate/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/decorate"],
      "options": {
        "outputPath": "coverage/libs/decorate",
        "jestConfig": "libs/decorate/jest.config.ts"
      }
    },
    "build": {
      "executor": "@pebula/nx-build-pipe:build",
      "options": {
        "task": {
          "type": "target",
          "target": "build-package"
        }
      }
    },
    "build-package": { },
    "package": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/decorate",
        "tsConfig": "libs/decorate/tsconfig.lib.json",
        "main": "libs/decorate/src/index.ts",
        "assets": ["libs/decorate/*.md"],
        "srcRootForCompilationRoot": "libs/decorate"
      }
    }
  }
}
