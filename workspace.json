{
  "version": 1,
  "projects": {
    "decorate": {
      "root": "libs/decorate",
      "sourceRoot": "libs/decorate/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/decorate/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/decorate"],
          "options": {
            "jestConfig": "libs/decorate/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/decorate",
            "tsConfig": "libs/decorate/tsconfig.lib.json",
            "packageJson": "libs/decorate/package.json",
            "main": "libs/decorate/src/index.ts",
            "assets": ["libs/decorate/*.md"],
            "srcRootForCompilationRoot": "libs/decorate"
          }
        },
        "version": {
          "builder": "@jscutlery/semver:version",
          "options": {
            "syncVersions": false
          }
        }
      }
    },
    "tom": {
      "root": "libs/tom",
      "sourceRoot": "libs/tom/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/tom/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/tom"],
          "options": {
            "jestConfig": "libs/tom/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/tom",
            "tsConfig": "libs/tom/tsconfig.lib.json",
            "packageJson": "libs/tom/package.json",
            "main": "libs/tom/src/index.ts",
            "assets": ["libs/tom/*.md"],
            "srcRootForCompilationRoot": "libs/tom"
          }
        },
        "benchmark": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "./node_modules/.bin/ts-node -r tsconfig-paths/register -P ./tsconfig.base.json -O \"{ \\\"module\\\": \\\"commonjs\\\", \\\"resolveJsonModule\\\": true }\" libs/tom/benchmarks/index.ts"
            ],
            "parallel": false
          }
        },
        "version": {
          "builder": "@jscutlery/semver:version",
          "options": {
            "syncVersions": false
          }
        },
        "apiExtractor": {
          "builder": "./dist/libs/nx-api-extractor:build",
          "options": {
            "extractorConfig": {
              "docModel": {
                "enabled": false
              },
              "apiReport": {
                "enabled": true,
                "reportFileName": "public-api.md",
                "reportFolder": "./",
                "reportTempFolder": "./temp"
              },
              "dtsRollup": {
                "enabled": true,
                "untrimmedFilePath": "",
                "betaTrimmedFilePath": "",
                "publicTrimmedFilePath": "./src/index-public.d.ts"
              },
              "tsdocMetadata": {
                "enabled": false
              },
              "messages": {
                "tsdocMessageReporting": {
                  "default": {
                    "logLevel": "none"
                  }
                },
                "extractorMessageReporting": {
                  "ae-missing-release-tag": {
                    "logLevel": "none"
                  },
                  "ae-unresolved-link": {
                    "logLevel": "none"
                  }
                }
              }
            }
          }
        }
      }
    },
    "decorate-docs": {
      "projectType": "application",
      "root": "apps/decorate-docs",
      "sourceRoot": "apps/decorate-docs/src",
      "architect": {
        "build": {
          "builder": "@nx-plus/docusaurus:browser",
          "options": {
            "outputPath": "dist/apps/decorate-docs"
          }
        },
        "serve": {
          "builder": "@nx-plus/docusaurus:dev-server",
          "options": {
            "port": 3000
          }
        }
      }
    },
    "tom-docs": {
      "projectType": "application",
      "root": "apps/tom-docs",
      "sourceRoot": "apps/tom-docs/src",
      "architect": {
        "build": {
          "builder": "@nx-plus/docusaurus:browser",
          "options": {
            "outputPath": "dist/apps/tom-docs"
          }
        },
        "serve": {
          "builder": "@nx-plus/docusaurus:dev-server",
          "options": {
            "port": 3000
          }
        }
      }
    },
