              {
                "input": "./libs/nx-api-extractor/src",
                "glob": "**/*.!(ts)",
                "output": "./src"
              },
              {
                "input": "./libs/nx-api-extractor",
                "glob": "executors.json",
                "output": "."
              }
            ]
