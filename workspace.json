          }
        }
      }
    },
    "touchstone-docs": {
      "projectType": "application",
      "root": "apps/touchstone-docs",
      "sourceRoot": "apps/touchstone-docs/src",
      "architect": {
        "build": {
          "builder": "@nx-plus/docusaurus:browser",
          "options": {
            "outputPath": "dist/apps/touchstone-docs"
          }
        },
        "serve": {
          "builder": "@nx-plus/docusaurus:dev-server",
          "options": {
