{
  "version": 1,
  "projects": {
    "docs-decorate": {
      "projectType": "application",
      "root": "apps/docs/decorate",
      "sourceRoot": "apps/docs/decorate/src",
      "architect": {
        "build": {
          "builder": "@nx-plus/docusaurus:browser",
          "options": {
            "outputPath": "dist/apps/docs/decorate"
          }
        },
        "serve": {
          "builder": "@nx-plus/docusaurus:dev-server",
          "options": {
            "port": 3000
          }
        }
      }
    },
    "docs-tom": {
      "projectType": "application",
      "root": "apps/docs/tom",
      "sourceRoot": "apps/docs/tom/src",
      "architect": {
        "build": {
          "builder": "@nx-plus/docusaurus:browser",
          "options": {
            "outputPath": "dist/apps/docs/tom"
          }
        },
        "serve": {
          "builder": "@nx-plus/docusaurus:dev-server",
          "options": {
            "port": 3000
          }
        }
      }
    },
    "docs-goosetyped": {
      "projectType": "application",
      "root": "apps/docs/goosetyped",
      "sourceRoot": "apps/docs/goosetyped/src",
      "architect": {
        "gh-pages": {
          "builder": "./dist/libs/nx-build-pipe:build",
          "options": {
            "task": {
              "type": "group",
              "composition": "sequence",
              "pipe": [
                {
                  "type": "target",
                  "target": "goosetyped:build"
                },
                {
                  "type": "runCommand",
                  "options": {
                    "commands": [
                      "./node_modules/.bin/api-documenter markdown -i ./dist/libs/goosetyped  -o ./apps/docs/goosetyped/docs/api"
                    ],
                    "parallel": false
                  }
                },
                {
                  "type": "target",
                  "target": "docs-goosetyped:build",
                  "options": {
                    "outputPath": "dist/gh-pages/goosetyped"
                  }
                }
              ]
            },
            "env": {
              "GH_PAGES_BUILD": true
            }
          }
        },
        "build": {
          "builder": "@nx-plus/docusaurus:browser",
          "options": {
            "outputPath": "dist/apps/docs/goosetyped"
          }
        },
        "serve": {
          "builder": "@nx-plus/docusaurus:dev-server",
          "options": {
            "port": 3000
          }
        }
      }
    },
